{
  "name": "database",
  "version": "0.0.0",
  "dependencies": {
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prisma": "5.3.1"
  },
  "scripts": {
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "db:generate": "dotenv -e ../../.env -- prisma generate",
    "start": "supabase status || supabase start",
    "reset": "supabase db reset || supabase start",
    "test": "supabase db test",
    "lint": "supabase db lint",
    "deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push"
  },
  "main": "./index.ts",
  "types": "./index.ts"
}
